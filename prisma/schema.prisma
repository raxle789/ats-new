}

model Certifications {
  id              Int          @id() @default(autoincrement()) @db.Int
  institutionName String       @map("institution_name") @db.NVarChar(510)
  issuedDate      DateTime     @map("issued_date") @db.Date
  certificateId   Int          @map("certificate_id") @db.Int
  candidateId     Int          @map("candidate_id") @db.Int
